
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="theme-color" content="#1a1a1a">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="dns-prefetch" href="//fonts.googleapis.com">
    <link rel="manifest" href="/manifest.json">
    <link rel="icon" type="image/png" href="/static/favicon/32x32.png">
    <link rel="stylesheet" href="/static/main.bundle.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700;900">
    <link rel="stylesheet" href="https://unpkg.com/prismjs@1.23.0/themes/prism-tomorrow.css">

    <meta name="description" content="{{ description or meta.description }}">
    <meta property="og:site_name" content="{{ meta.title }}">
    <meta property="og:title" content="{{ title or meta.title }}">
    <meta property="og:description" content="{{ description  or meta.description }}">
    <meta property="og:url" content="{{ absolutePageUrl }}">
    <meta property="og:locale" content="{{ meta.locale }}">
    {%- if image -%}
        <meta property="og:image" content="{{ image | url | absoluteUrl(meta.url) }}">
    {%- endif %}

    <title>{{ title }}</title>